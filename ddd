    def run_simulation(self, data_paid, sigma_j, dev, sd, e_values,
                       exposure_one_year_np, wagi_trimmed,
                       ilosc_dop_wsp_CL, Poz_LR, il_ogon,
                       ultimate_param_resrisk, net_to_gross,
                       sim_total=100_000, batch_sim=10_000, main_seed=202206011):

        return simulate_reserving_batched_numba(
            data_paid,
            sigma_j, dev, sd, e_values, exposure_one_year_np, wagi_trimmed,
            ilosc_dop_wsp_CL, Poz_LR, il_ogon,
            ultimate_param_resrisk, net_to_gross,
            sim_total, batch_sim, main_seed
        )
