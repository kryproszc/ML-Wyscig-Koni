if not run_inside_numba:
    a, b = compute_curve(dev_choose, sd_choose, ind_choose)

def compute_curve(dev_choose, sd_choose, ind_choose):
    a, b = fit_curve(
        dev_choose,
        sd_choose,
        np.array(ind_choose) + 1,
        'factor_CL',
        -0.99
    )
    return float(a), float(b)
