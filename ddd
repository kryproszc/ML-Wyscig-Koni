$node = (where.exe node)
New-NetFirewallRule -DisplayName "Allow Node.js inbound" `
  -Direction Inbound -Program $node -Action Allow -Profile Domain,Private,Public


Invoke-WebRequest http://localhost:3000 -UseBasicParsing
Invoke-WebRequest http://172.25.210.87:3000 -UseBasicParsing


netstat -ano | findstr :3000


